{% extends 'base.html.twig' %}

{% block title %}TypeInterview index{% endblock %}

{% block body %}
    <h1>TypeInterview index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_interview in type_interviews %}
            <tr>
                <td>{{ type_interview.id }}</td>
                <td>{{ type_interview.name }}</td>
                <td>{{ type_interview.description }}</td>
                <td>
                    <a href="{{ path('app_type_interview_show', {'id': type_interview.id}) }}">show</a>
                    <a href="{{ path('app_type_interview_edit', {'id': type_interview.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_interview_new') }}">Create new</a>
{% endblock %}
