<form method="post" action="{{ path('app_interview_delete', {'id': interview.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet entretien ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ interview.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
